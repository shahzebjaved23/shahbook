<div>
	<h1><%= @photos.count == 0 ? @user.first_name + " has no photos to show" : @user.first_name+"'s has #{@photos.count} photos"%></h1>

	<div class="row">
		<div class="col-md-4">
			<% @photos.each do |photo| %>
			<div class="thumbnail">
				<%= link_to image_tag(photo.picture.url),user_profile_photo_path(@user.id,photo.id)%>
				
			</div>
			<% end %>
		</div>
	</div>
</div>